# .env.example
# Copy this file to .env and update with your actual values

# =============================================================================
# AWS Configuration
# =============================================================================
AWS_ACCOUNT_ID=123456789012
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-here
AWS_SECRET_ACCESS_KEY=your-secret-key-here
AWS_DEFAULT_REGION=us-east-1

# S3 Configuration
S3_DATA_BUCKET=multi-tenant-data
S3_BACKUP_BUCKET=multi-tenant-backups

# =============================================================================
# Databricks Configuration
# =============================================================================
DATABRICKS_ACCOUNT_ID=your-databricks-account-id
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=dapi-your-token-here
DATABRICKS_WAREHOUSE_ID=your-warehouse-id

# Databricks Workspace Configuration
DATABRICKS_WORKSPACE_URL=https://your-workspace.cloud.databricks.com
DATABRICKS_CLUSTER_ID=your-cluster-id

# =============================================================================
# Database Configuration
# =============================================================================
# Primary Database (PostgreSQL)
DATABASE_URL=postgresql://dbadmin:password123@localhost:5432/multitenantdb
DB_HOST=localhost
DB_PORT=5432
DB_NAME=multitenantdb
DB_USER=dbadmin
DB_PASSWORD=password123

# Service-specific Database URLs
PIPELINE_DB_URL=postgresql://dbadmin:password123@localhost:5432/pipeline_db
CATALOG_DB_URL=postgresql://dbadmin:password123@localhost:5432/catalog_db
CONNECTOR_DB_URL=postgresql://dbadmin:password123@localhost:5432/connector_db
DQ_DB_URL=postgresql://dbadmin:password123@localhost:5432/dq_db

# =============================================================================
# Redis Configuration
# =============================================================================
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# =============================================================================
# Security Configuration
# =============================================================================
# JWT Configuration
JWT_SECRET_KEY=your-super-secret-jwt-key-change-this-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# Encryption Keys
ENCRYPTION_KEY=your-32-character-encryption-key-here
FERNET_KEY=your-fernet-key-for-airflow-here

# API Keys
API_SECRET_KEY=your-api-secret-key-here

# =============================================================================
# Feature Flags
# =============================================================================
UNITY_CATALOG_ENABLED=true
ICEBERG_ENABLED=true
MULTI_TENANT_MODE=true
DEBUG_MODE=false
ENABLE_METRICS=true
ENABLE_TRACING=true

# =============================================================================
# Environment Settings
# =============================================================================
ENVIRONMENT=development
LOG_LEVEL=INFO
PYTHONPATH=/app

# Application URLs
API_BASE_URL=http://localhost
FRONTEND_URL=http://localhost:3000

# =============================================================================
# Monitoring and Observability
# =============================================================================
# Prometheus
PROMETHEUS_URL=http://localhost:9090

# Grafana
GRAFANA_URL=http://localhost:3001
GRAFANA_ADMIN_PASSWORD=admin123

# ElasticSearch
ELASTICSEARCH_URL=http://localhost:9200

# =============================================================================
# Third-Party Integrations
# =============================================================================
# Slack (for notifications)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/webhook/url
SLACK_CHANNEL=#data-engineering

# Email (SMTP)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
SMTP_FROM_EMAIL=noreply@yourcompany.com

# =============================================================================
# Organization Configuration
# =============================================================================
# Default organization settings
DEFAULT_ORG_NAME=default
DEFAULT_ORG_DISPLAY_NAME=Default Organization

# Organization-specific configurations can be added here
# ORG_FINANCE_DISPLAY_NAME=Finance Department
# ORG_FINANCE_COST_CENTER=FIN-001
# ORG_FINANCE_MAX_DBU_PER_HOUR=100

# =============================================================================
# Resource Quotas and Limits
# =============================================================================
DEFAULT_MAX_DBU_PER_HOUR=50
DEFAULT_MAX_STORAGE_GB=1000
DEFAULT_MAX_API_CALLS_PER_MINUTE=100

# =============================================================================
# Development Tools
# =============================================================================
# PgAdmin
PGADMIN_DEFAULT_EMAIL=admin@example.com
PGADMIN_DEFAULT_PASSWORD=admin123

# Redis Commander
REDIS_COMMANDER_PORT=8081

# Jupyter
JUPYTER_TOKEN=admin123
JUPYTER_PORT=8888

# MinIO (S3-compatible storage for local development)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123
MINIO_PORT=9000
MINIO_CONSOLE_PORT=9001

# =============================================================================
# Message Queue Configuration
# =============================================================================
# RabbitMQ
RABBITMQ_DEFAULT_USER=admin
RABBITMQ_DEFAULT_PASS=admin123
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_MANAGEMENT_PORT=15672

# =============================================================================
# Airflow Configuration (if using)
# =============================================================================
AIRFLOW_DATABASE_URL=postgresql+psycopg2://dbadmin:password123@localhost:5432/airflow_db
AIRFLOW_WEBSERVER_PORT=8080
AIRFLOW_ADMIN_USERNAME=admin
AIRFLOW_ADMIN_PASSWORD=admin123
AIRFLOW_ADMIN_EMAIL=admin@example.com

# =============================================================================
# API Service Ports
# =============================================================================
PIPELINE_SERVICE_PORT=8000
CATALOG_SERVICE_PORT=8001
CONNECTOR_SERVICE_PORT=8002
DQ_SERVICE_PORT=8003
UI_SERVICE_PORT=3000

# =============================================================================
# Load Balancer Configuration
# =============================================================================
NGINX_PORT=80
NGINX_SSL_PORT=443

# =============================================================================
# Container Configuration
# =============================================================================
# Docker compose project name
COMPOSE_PROJECT_NAME=multi-tenant-ingestion

# Container restart policy
RESTART_POLICY=unless-stopped

# =============================================================================
# Backup and Recovery
# =============================================================================
BACKUP_SCHEDULE=0 2 * * *  # Daily at 2 AM
BACKUP_RETENTION_DAYS=30
BACKUP_S3_BUCKET=multi-tenant-backups

# =============================================================================
# Performance Tuning
# =============================================================================
# Database connection pooling
DB_POOL_SIZE=20
DB_MAX_OVERFLOW=30

# Worker processes
WORKER_PROCESSES=4
WORKER_CONNECTIONS=1000

# Cache settings
CACHE_TTL_SECONDS=300
CACHE_MAX_SIZE=1000

# =============================================================================
# Testing Configuration
# =============================================================================
TEST_DATABASE_URL=postgresql://dbadmin:password123@localhost:5432/test_db
PYTEST_TIMEOUT=300

# =============================================================================
# CI/CD Configuration
# =============================================================================
# GitHub Actions
GITHUB_TOKEN=your-github-token
GITHUB_REPOSITORY=your-org/multi-tenant-ingestion

# Docker Registry
DOCKER_REGISTRY=your-registry.com
DOCKER_IMAGE_TAG=latest

# =============================================================================
# Compliance and Governance
# =============================================================================
# Data retention policies
DATA_RETENTION_DAYS=2555  # 7 years
LOG_RETENTION_DAYS=90

# Audit settings
ENABLE_AUDIT_LOGGING=true
AUDIT_LOG_LEVEL=INFO

# =============================================================================
# Custom Application Settings
# =============================================================================
# Pipeline execution settings
DEFAULT_PIPELINE_TIMEOUT=3600  # 1 hour
MAX_CONCURRENT_PIPELINES=10
PIPELINE_RETRY_ATTEMPTS=3

# Data quality settings
DQ_CHECK_TIMEOUT=1800  # 30 minutes
DQ_SAMPLE_SIZE=10000
DEFAULT_DQ_THRESHOLD=95.0

# Connector settings
CONNECTOR_TIMEOUT=300  # 5 minutes
MAX_PREVIEW_ROWS=1000

# Catalog settings
CATALOG_SYNC_INTERVAL=3600  # 1 hour
ENABLE_AUTO_SCHEMA_DETECTION=true

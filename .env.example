# Multi-Tenant Data Ingestion Framework - Environment Configuration

# Environment
ENVIRONMENT=development
PROJECT_NAME=multi-tenant-ingestion
DEBUG=false

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# Database Configuration
DATABASE_URL=postgresql://app_user:password@localhost:5432/multi_tenant_ingestion
REDIS_URL=redis://localhost:6379/0

# Databricks Configuration
DATABRICKS_ACCOUNT_ID=your-databricks-account-id
DATABRICKS_WORKSPACE_URL=https://your-workspace.cloud.databricks.com
DATABRICKS_TOKEN=dapi-your-token-here

# Security
JWT_SECRET_KEY=your-jwt-secret-key-minimum-32-characters
ENCRYPTION_KEY=your-encryption-key-minimum-32-characters

# Feature Flags
UNITY_CATALOG_ENABLED=true
ICEBERG_ENABLED=true
MULTI_TENANT_MODE=true

# API Configuration
ALLOWED_ORIGINS=http://localhost:3000,https://your-domain.com
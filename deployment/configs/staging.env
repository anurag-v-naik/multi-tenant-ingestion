# Staging Environment Configuration

ENVIRONMENT=staging
DEBUG=false

# AWS Configuration (replace with actual values)
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# Project Configuration
PROJECT_NAME=multi-tenant-ingestion

# Database (will be replaced by deployment script with actual RDS endpoint)
DATABASE_URL=postgresql://app_user:REPLACE_PASSWORD@REPLACE_RDS_ENDPOINT:5432/multi_tenant_ingestion

# Redis (will be replaced by deployment script with actual Redis endpoint)
REDIS_URL=redis://REPLACE_REDIS_ENDPOINT:6379/0

# Databricks (replace with actual staging values)
DATABRICKS_WORKSPACE_URL=https://staging-workspace.cloud.databricks.com
DATABRICKS_TOKEN=REPLACE_WITH_STAGING_TOKEN

# Security (replace with actual staging values)
JWT_SECRET_KEY=REPLACE_WITH_STAGING_JWT_SECRET
ENCRYPTION_KEY=REPLACE_WITH_STAGING_ENCRYPTION_KEY

# Feature Flags
UNITY_CATALOG_ENABLED=true
ICEBERG_ENABLED=true
MULTI_TENANT_MODE=true

# API Configuration
ALLOWED_ORIGINS=https://staging.your-domain.com

# Development Environment Configuration

ENVIRONMENT=development
DEBUG=true

# AWS Configuration (using default/local values for development)
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=123456789012

# Project Configuration
PROJECT_NAME=multi-tenant-ingestion

# Database (using local PostgreSQL)
DATABASE_URL=postgresql://app_user:password@localhost:5432/multi_tenant_ingestion

# Redis (using local Redis)
REDIS_URL=redis://localhost:6379/0

# Databricks (using mock/test values)
DATABRICKS_WORKSPACE_URL=http://localhost:8080/mock-databricks
DATABRICKS_TOKEN=mock-token-for-development

# Security (use test values for development)
JWT_SECRET_KEY=development-jwt-secret-key-32-chars-min
ENCRYPTION_KEY=development-encryption-key-32-chars

# Feature Flags
UNITY_CATALOG_ENABLED=true
ICEBERG_ENABLED=true
MULTI_TENANT_MODE=true

# API Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080
